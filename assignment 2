#include<iostream>
using namespace std;
int main()
{
    int a;
    cout <<"Enter a number" <<endl;
    cin >> a;
    int temp=0;
    for(int i=2;i<a;i++)
    {
        if(a%i==0)
        temp ++;
    }
    if(temp==0)
    cout<<"It is a prime number"<<endl;
    else
    cout<<"Not a prime number"<<endl;
}
